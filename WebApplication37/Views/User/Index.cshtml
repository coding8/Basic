@*@model IEnumerable<WebApplication37.Models.ListUser>*@
@model IEnumerable<WebApplication37.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("添加新用户", "Create", null, new { @class = "btn btn-primary" })
    <span class="badge">@ViewBag.UserNum</span>
</p>
<table class="table table-condensed table-bordered">
    <tr>
        <th>@Html.DisplayNameFor(model=>model.Id)</th>
        <th>@Html.DisplayNameFor(model => model.UserName)</th>
        <th>@Html.DisplayNameFor(model => model.LockoutEnabled)</th>
        <th>@Html.DisplayNameFor(model => model.LockoutEndDateUtc)</th>
        <th>@Html.DisplayNameFor(model => model.AccessFailedCount)</th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            <td>@Html.DisplayFor(modelItem => item.UserName)</td>
            <td>@Html.DisplayFor(modelItem => item.LockoutEnabled)</td>
            <td>@Html.DisplayFor(modelItem => item.LockoutEndDateUtc)</td>
            <td>@Html.DisplayFor(modelItem => item.AccessFailedCount)</td>
            <td>
                @Html.ActionLink("编辑", "Edit", new { id = item.Id }) |
                @Html.ActionLink("详细", "Details", new { id = item.Id }) |
                @Html.ActionLink("删除", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>